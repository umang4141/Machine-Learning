list_1 = [1, 2, -3, 4.5, 'indian']
print(list_1)



import pandas as pd	
series1 = pd.Series(list_1)
print(series1)



empty_s = pd.Series([])
print(empty_s)


series2 = pd.Series([1,2,3,4,5])
print(series2)

series2 = pd.Series([1,2,3,4,5], index = ['a', 'b', 'c', 'd', 'e'])
print(series2)




s5 = pd.Series([1,2,3,4,5])
s6 = pd.Series([1,2,3,4,5])
 
a = s5 + s6
print(a)
